{
	"name": "vscode-textobjects",
	"publisher": "rodrigoscola",
	"author": {
		"name": "Rodrigo Scola",
		"url": "https://github.com/RodrigoScola"
	},
	"displayName": "vscode text objects",
	"keywords": [
		"select",
		"text",
		"keybinds",
		"shortcuts",
		"movement"
	],
	"bugs": {
		"url": "https://github.com/RodrigoScola/vscode-text-objects/issues"
	},
	"homepage": "https://github.com/RodrigoScola/vscode-text-objects",
	"description": "inspired by tree sitter. this is the vscode implementation of object selection",
	"repository": {
		"type": "git",
		"url": "https://github.com/RodrigoScola/vscode-text-objects"
	},
	"version": "0.0.22",
	"engines": {
		"vscode": "^1.93.0"
	},
	"categories": [
		"Other",
		"Keymaps"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact",
		"onLanguage:json",
		"onLanguage:jsonc"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"title": "Vscode Text Objects",
			"properties": {
				"vscode-textobjects.selection.groupElements": {
					"title": "Group elements",
					"description": "group some nodes such as parameters so you can select them all at once",
					"default": false,
					"type": "boolean"
				},
				"vscode-textobjects.logs.bugsPath": {
					"type": "string",
					"description": " the selection bug file, for developers only "
				}
			}
		},
		"keybindings": [
			{
				"command": "vscode-textobjects.select.next.outer.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+o ctrl+p",
				"mac": "cmd+o cmd+p"
			},
			{
				"command": "vscode-textobjects.select.previous.outer.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+shift+o ctrl+shift+p",
				"mac": "cmd+shift+o cmd+shift+p"
			},
			{
				"command": "vscode-textobjects.select.next.inner.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+i ctrl+p",
				"mac": "cmd+i cmd+p"
			},
			{
				"command": "vscode-textobjects.select.previous.inner.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+shift+i ctrl+shift+p",
				"mac": "cmd+shift+i cmd+shift+p"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+n ctrl+p",
				"mac": "cmd+n cmd+p"
			},
			{
				"command": "vscode-textobjects.goTo.previous.outer.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+shift+p ctrl+shift+p",
				"mac": "cmd+shift+n cmd+shift+p"
			},
			{
				"command": "vscode-textobjects.goTo.next.inner.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+shift+n ctrl+p",
				"mac": "cmd+n cmd+p"
			},
			{
				"command": "vscode-textobjects.goTo.previous.inner.parameters",
				"when": "editorTextFocus",
				"key": "ctrl+n ctrl+p",
				"mac": "cmd+n cmd+p"
			}
		],
		"commands": [
			{
				"command": "vscode-textobjects.goTo.next.outer.call",
				"title": "Go to next function call",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.function",
				"title": "go to the next function",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.conditional",
				"title": "go to the next if statement",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.loop",
				"title": "go to the next if statement",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.rhs",
				"title": "go to the next variable assignments",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.variable",
				"title": "go to the next variable ",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.string",
				"title": "go to the next string",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.array",
				"title": "go to the next array",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.object",
				"title": "go to the next object",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.parameters",
				"title": "go to the next parameter",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.type",
				"title": "go to the next type",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.comments",
				"title": "go to the next comment",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.goTo.next.outer.class",
				"title": "go to the next class",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.previous.outer.loop",
				"title": "selects the previous loop",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.previous.outer.function",
				"title": "selects the previous function",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.comment",
				"title": "selects the nearest comment",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.call",
				"title": "select a function or operation call",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.call",
				"title": "select a function or operation call",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.treeSitter",
				"title": "views the treesitter range",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.bugFile",
				"title": "adds the current file or selection to bugs",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.string",
				"title": "select the nearest inner string",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.string",
				"title": "select the nearest string",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.object",
				"title": "select the nearest object",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.array",
				"title": "select the nearest array",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.class",
				"title": "select the inner class",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.class",
				"title": "select the nearest class",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.variable",
				"title": "select variables",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.rhs",
				"title": "select variable assignments",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.type",
				"title": "select inner type",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.type",
				"title": "select type ",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.parameters",
				"title": "Select parameters",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.parameters",
				"title": "Select parameter content",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.conditional",
				"title": "Select If statement",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.conditional",
				"title": "Select If statement content",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.loop",
				"title": "Select loop",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.loop",
				"title": "Select loop content",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.outer.function",
				"title": "Select Function",
				"enablement": "editorTextFocus"
			},
			{
				"command": "vscode-textobjects.select.next.inner.function",
				"title": "Select function content",
				"enablement": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p . --outDir out && cp -r ./parsers ./out/parsers",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/vscode": "^1.93.0",
		"@typescript-eslint/eslint-plugin": "^8.3.0",
		"@typescript-eslint/parser": "^8.3.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"esbuild": "^0.23.1",
		"eslint": "^9.9.1",
		"npm-run-all": "^4.1.5",
		"tree-sitter-cli": "^0.23.0",
		"tree-sitter-javascript": "^0.23.0",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"web-tree-sitter": "^0.23.0"
	}
}
